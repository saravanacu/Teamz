{
  "name": "basketball-clubs-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:client:dev\" \"npm:server:dev\"",
    "start": "node server/index.js",
    "client:install": "cd client && npm install",
    "server:install": "cd server && npm install",
    "install:all": "npm run client:install && npm run server:install"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "maplibre-gl": "^5.9.0"
  }
}
